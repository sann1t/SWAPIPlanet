<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.swapiapi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".categoryActivity.peoples.PeopleActivityInfo"></activity>
        <activity android:name=".categoryActivity.planets.PlanetActivityInfo"></activity>
        <activity android:name=".categoryActivity.species.SpecieActivityInfo"></activity>
        <activity android:name=".categoryActivity.starships.StarShipActivityInfo"></activity>
        <activity android:name=".categoryActivity.vehicles.VehicleActivityInfo"></activity>
        <activity android:name=".categoryActivity.starships.StarShipActivity" />
        <activity android:name=".categoryActivity.species.SpecieActivity" />
        <activity android:name=".categoryActivity.peoples.PeopleActivity" />
        <activity android:name=".categoryActivity.vehicles.VehicleActivity" />
        <activity android:name=".categoryActivity.planets.PlanetActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>